{
    "name": "pavelz/laravel-doctrine-rest",
    "description": "Doctrine Rest - Laravel implementation",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "post-install-cmd": "./vendor/phpunit/phpunit/phpunit --config phpunit.xml"
    },
    "authors": [
        {
            "name": "PavelZ",
            "email": "r3volut1oner@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pz\\LaravelDoctrine\\JsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "tests/laravel/App",
            "Tests\\": "tests/unit",
            "Database\\Migrations\\": "tests/database/migrations",
            "Database\\Seeders\\": "tests/database/seeders"
        }
    },
    "require-dev": {
        "laravel-doctrine/orm": "^1.7",
        "laravel-doctrine/acl": "^1.5",
        "laravel-doctrine/migrations": "^2.3",
        "doctrine/annotations": "^1.13",
        "laravel/laravel": "^8.0",
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "require": {
        "league/fractal": "^0.20.1"
    }
}
